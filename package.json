{
  "name": "com.accedia.training",
  "version": "1.0.0",
  "description": "Week 1-6 project",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css:scss": "node-sass --output-style compressed -o dist src/assets/style/style.scss",
    "css:autoprefixer": "postcss -u autoprefixer -r dist/*.css",
    "css:lint": "stylelint src/assets/style/*.scss --syntax scss || true",
    "build:css": "npm run css:lint && npm run css:scss && npm run css:autoprefixer",
    "watch:css": "onchange \"src/assets/style\" -- npm run build:css",
    "serve": "browser-sync start --server \"dist\" --files \"dist\"",
    "watch": "run-p serve watch:css",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaypleshkov/we-lvlup-9gag.git"
  },
  "keywords": [
    "9gag",
    "9gag-clone",
    "accedia",
    "intern"
  ],
  "author": "Nikolay Pleshkov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nikolaypleshkov/we-lvlup-9gag/issues"
  },
  "homepage": "https://github.com/nikolaypleshkov/we-lvlup-9gag#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "browser-sync": "^2.27.7",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.1.0",
    "stylelint": "^14.5.3",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
